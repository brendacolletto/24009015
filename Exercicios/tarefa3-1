A-) #include <stdio.h>

int main() {
    double n = 4000000000;
   printf("%f\n", n);
    return 0;
}

B-) #include <stdio.h>

int main() {
    int a;
    a = 0; // Inicialização de 'a'
    printf("%d\n", a);
    return 0;
}

C-) #include <stdio.h>

int main() {
    printf("%d", 4);
    printf("%d\n", 2); 
    return 0;
}

D-) #include <stdio.h>

int main() {
    int a;
    scanf("%d", &a); 
    printf("%d\n", a);
    return 0;
}
